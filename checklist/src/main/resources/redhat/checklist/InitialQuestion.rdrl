package redhat.checklist;

import java.lang.Number;

rule "InitialQuestion"
	dialect "mvel"
	when
		exists (Questionare( contextId : id)) 
	then
		QandAContext qaContext = new  QandAContext( );
		qaContext.addQuestion("1", "Are the building contents being taken as collateral by the borrower or related entity? Either by UCC or stated within the mortgage.");
		 kcontext.insert(qaContext);
end
